	int angka = 10;
	int i, j, k;
	for (i = 1; i <= angka; i++)
	{
		for (j = 1; j <= i; j++)
		{
			Console.Write("");
		}
		for (k = 1; k <= i; k++)
		{
			Console.Write("*");
		}
		Console.WriteLine("");
	}

}

